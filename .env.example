# .env.example

# Your Twitch application client ID
CLIENT_ID=your_client_id_here

# The OAuth Redirect URI for your Twitch application
REDIRECT_URI=your_redirect_uri_here

# Your Twitch username
TWITCH_USERNAME=your_twitch_username_here

# Your OAuth token (this should be set dynamically after authentication)
OAUTH_TOKEN=your_oauth_token_here